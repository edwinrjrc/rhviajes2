

select * from negocio."Direccion" where principal = 'S';

select p.*
  from negocio."Direccion" d
 inner join negocio."PersonaDireccion" pd on pd.iddireccion = d.id
 inner join negocio."Persona" p on p.id = pd.idpersona and p.idtipopersona = 3
 inner join negocio."TelefonoDireccion" td on td.iddireccion = d.id
 inner join negocio."PersonaContactoProveedor" on pcp on 
 where manzana is not null
   and NOMBREvia not like '%SIN%';
   

   
select * from negocio."PersonaContactoProveedor";
SELECT * FROM negocio."TelefonoDireccion"
 

select * from negocio."Persona" where id = 4;

select * from negocio."ArchivoCargado";

select * from negocio."DocumentoAdjuntoServicio";

select * from negocio."PersonaAdjuntos";


select * from negocio."Persona" where numerodocumento = '40069216';

select * from negocio."PersonaDireccion" where idpersona = 328;

select * from negocio."Direccion" where id in (855,854);